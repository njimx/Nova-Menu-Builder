{
    "name": "njimx/nova-menu-builder",
    "description": "Menu Builder Tool for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "menu",
        "builder",
        "infinety"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Lagarda",
            "email": "eric@infinety.es",
            "role": "Developer"
        },
        {
            "name": "InfinetyEs",
            "email": "apps@infinety.es",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0",
        "inertiajs/inertia-laravel": "^0.5.4"
    },
    "autoload": {
        "psr-4": {
            "Infinety\\MenuBuilder\\": "src/"
        },
        "files": [
          "src/Http/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infinety\\MenuBuilder\\MenuBuilderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
